<?xml version="1.0" encoding="UTF-8"?>
<bindings xmlns="http://java.sun.com/xml/ns/jaxb" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	xsd:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"
	version="2.1">
	<globalBindings>
		<xjc:superClass name="de.invesdwin.common.beans.AValueObject" />
		<xjc:serializable uid="1" />
		<!-- primitive types are not good because they are initialized with default 
			values and thus the required check does not work properly -->
		<javaType name="java.lang.Byte" xmlType="xsd:byte" />
		<javaType name="java.lang.Short" xmlType="xsd:unsignedByte" />
		<javaType name="java.lang.Short" xmlType="xsd:short" />
		<javaType name="java.lang.Integer" xmlType="xsd:unsignedShort" />
		<javaType name="java.lang.Integer" xmlType="xsd:int" />
		<javaType name="java.lang.Long" xmlType="xsd:unsignedInt" />
		<javaType name="java.lang.Long" xmlType="xsd:long" />
		<javaType name="java.lang.Float" xmlType="xsd:float" />
		<javaType name="java.lang.Double" xmlType="xsd:double" />
		<javaType name="java.lang.Boolean" xmlType="xsd:boolean" />
		<javaType name="de.invesdwin.util.math.decimal.Decimal"
			xmlType="xsd:decimal"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseDecimal"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printDecimal" />
		<!-- XmlGregorianCalendar is quity unhandy, thus we replace it with Calendar -->
		<javaType name="de.invesdwin.util.time.fdate.FDate"
			xmlType="xsd:time"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseTime"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printTime" />
		<javaType name="de.invesdwin.util.time.fdate.FDate"
			xmlType="xsd:date"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseDate"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printDate" />
		<javaType name="de.invesdwin.util.time.fdate.FDate"
			xmlType="xsd:dateTime"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseDateTime"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printDateTime" />
		<javaType name="de.invesdwin.util.time.fdate.FDate"
			xmlType="xsd:gDay"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseGDay"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printGDay" />
		<javaType name="de.invesdwin.util.time.fdate.FDate"
			xmlType="xsd:gMonth"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseGMonth"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printGMonth" />
		<javaType name="de.invesdwin.util.time.fdate.FDate"
			xmlType="xsd:gMonthDay"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseGMonthDay"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printGMonthDay" />
		<javaType name="de.invesdwin.util.time.fdate.FDate"
			xmlType="xsd:gYear"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseGYear"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printGYear" />
		<javaType name="de.invesdwin.util.time.fdate.FDate"
			xmlType="xsd:gYearMonth"
			parseMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.parseGYearMonth"
			printMethod="de.invesdwin.util.internal.jaxb.DatatypeConverter.printGYearMonth" />
	</globalBindings>

</bindings>
